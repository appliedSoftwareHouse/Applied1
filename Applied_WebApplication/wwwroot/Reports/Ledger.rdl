  <DataSets>
    <DataSet Name="dsname_Ledger">
      <Query>
        <DataSourceName>ds_Ledger</DataSourceName>
        <CommandText>SELECT * FROM (SELECT 
[L].*,
[A].[TITLE] AS [AccountTitle], 
[C].[TITLE] AS [CompanyName], 
[E].[TITLE] AS [EmployeeName], 
[P].[TITLE] AS [ProjectTitle], 
[I].[TITLE]  AS [StockTitle] 
FROM [Ledger] [L] 
LEFT JOIN [COA]       [A] ON [A].[ID] = [L].[COA] 
LEFT JOIN [Customers] [C] ON [C].[ID] = [L].[CUSTOMER] 
LEFT JOIN [Employees] [E] ON [E].[ID] = [L].[EMPLOYEE] 
LEFT JOIN [Project]   [P] ON [P].[ID] = [L].[PROJECT] 
LEFT JOIN [Inventory] [I] ON [I].[ID] = [L].[INVENTORY]
) AS [Ledger]
WHERE [COA] = 2</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
